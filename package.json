{
  "name": "ssvep-paradigm",
  "productName": "ssvep-paradigm",
  "version": "0.1.2",
  "description": "使用electron编写的博睿康设备兼容的SSVEP范式",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "install": "electron-rebuild"
  },
  "repository": "https://github.com/puresox/SSVEP-paradigm",
  "keywords": [
    "Electron"
  ],
  "author": {
    "name": "puresox",
    "email": "puresox@163.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "ignore": [
          "appData"
        ],
        "icon": "./src/public/icon.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "win32"
          ],
          "config": {
            "name": "ssvep_paradigm"
          }
        }
      ]
    }
  },
  "dependencies": {
    "@serialport/parser-inter-byte-timeout": "^9.0.7",
    "lodash": "^4.17.21",
    "mousetrap": "^1.6.5",
    "serialport": "^9.0.7"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "12.0.5",
    "electron-rebuild": "^2.3.5",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "prettier": "2.2.1"
  }
}
