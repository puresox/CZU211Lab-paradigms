{
  "name": "czu211lab-paradigms",
  "productName": "CZU211Lab-paradigms",
  "version": "0.2.1",
  "description": "使用electron编写的博睿康设备兼容的范式",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "install": "electron-rebuild"
  },
  "repository": "https://github.com/puresox/CZU211Lab-paradigms",
  "keywords": [
    "Electron"
  ],
  "author": {
    "name": "puresox",
    "email": "puresox@163.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "ignore": [
          "appData"
        ],
        "icon": "./src/public/icon.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "win32"
          ],
          "config": {
            "name": "CZU211Lab-paradigms"
          }
        }
      ]
    }
  },
  "dependencies": {
    "@serialport/parser-inter-byte-timeout": "^9.0.7",
    "lodash": "^4.17.21",
    "mousetrap": "^1.6.5",
    "serialport": "^9.2.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.61",
    "@electron-forge/maker-zip": "6.0.0-beta.61",
    "electron": "^14.0.0",
    "electron-rebuild": "^3.2.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "prettier": "^2.4.0"
  }
}
